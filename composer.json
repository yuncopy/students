        }
    },
    "require": {
        "bramus/router": "~1.2",
        "doctrine/dbal": "^2.6",
        "illuminate/database": "5.4.*",
        "illuminate/events": "5.4.*",
        "illuminate/pagination": "5.4.*",
        "livecontrol/eloquent-datatable": "^0.1.5",
        "monolog/monolog": "^1.23",
