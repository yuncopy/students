        "phpoffice/phpexcel": "^1.8",
        "twig/twig": "1.35.0"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
